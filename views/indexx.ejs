<body>
    <div class="frame">
        <form action="/upload" method="post" enctype="multipart/form-data">
        <div class="center">
            <div class="title">
                <h1>click the below icon to upload</h1>
            </div>
            
            <div class="dropzone">
                <img src="http://100dayscss.com/codepen/upload.svg" class="upload-icon" />
                <input type="file" id="image" class="upload-input" name='image' multiple required/>
                <ul>
                    <!-- <li id="myImage"> </li> -->
                </ul>

            </div>
            
            <button type="submit" class="btn" onclick="getValue()">Upload image</button>
            <h1> <%- Error %> </h1>
            <!-- <h1> <%#- Success %> </h1> -->
            <!-- <h1> <%#- dataSaved %> </h1> -->




            
            
        </div>
        </form>
    </div>

